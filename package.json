{
  "name": "house-mcp-manager",
  "version": "1.0.0",
  "description": "Universal MCP server manager for AI coding agents - part of the House suite of Claude Code tools",
  "main": "dist/cli.js",
  "type": "module",
  "bin": {
    "house-mcp-manager": "./dist/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/houseworthe/house-mcp-manager.git"
  },
  "bugs": {
    "url": "https://github.com/houseworthe/house-mcp-manager/issues"
  },
  "homepage": "https://github.com/houseworthe/house-mcp-manager#readme",
